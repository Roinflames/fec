<!-- Docker -->
docker compose down
docker compose up -d
docker stop $(docker ps -q)
docker stop en windows:
    for /f "tokens=*" %i in ('docker ps -q') do docker stop %i

<!-- Backend -->
php artisan --version
Laravel Framework 12.14.1

